(cloudflare) {
    tls{
        dns cloudflare {env.CLOUDFLARE_API_TOKEN}
        resolvers 1.1.1.1
    }
}
{env.BASE_DOMAIN} {
	respond <<HTML
		<html>
		  <head><title>Foo</title></head>
		  <body>Foo</body>
		</html>
		HTML 200
}
freshrss.{env.BASE_DOMAIN} {
    reverse_proxy http://{env.DOCKER_LXC_IP}:8080
    import cloudflare
}
